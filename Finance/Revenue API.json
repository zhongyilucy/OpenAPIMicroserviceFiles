{
  "openapi": "3.0.0",
  "info": {
    "description": "This API retrieves the information related to the revenue of users.",
    "version": "1.4.0",
    "title": "Revenue API",
    "license": {
      "name": "Git Repo",
      "url": "https://github.com/cseteaching-unsw-edu-au/se2021-23t1-einvoicing-api-f12a-gobgobgoo-creation-api"
    }
  },
  "servers": [
    {
      "description": "Invoice Creation API",
      "url": "http://deploy-s2-4-env.eba-fxeq7rku.ap-southeast-2.elasticbeanstalk.com"
    }
  ],
  "tags": [
    {
      "name": "Invoice",
      "description": "Everything about your Invoice",
      "externalDocs": {
        "description": "Find out more",
        "url": "http://swagger.io"
      }
    },
    {
      "name": "Admins",
      "description": "Everything about admin"
    }
  ],
  "paths": {
     "/revenue": {
      "get": {
        "tags": [
          "Admins"
        ],
        "summary": "Get revenue for authenticated user",
        "description": "Returns the total revenue generated by the authenticated user.",
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "revenue": {
                      "type": "number"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Invalid token"
          }
        }
      }
    },
    "/revenue/report": {
      "get": {
        "summary": "Retrieves information regarding revenue per month for the entire year",
        "tags": [
          "Admins"
        ],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation and return of revenue information"
          },
          "404": {
            "description": "Invalid token"
          }
        }
      }
    }
  },
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}